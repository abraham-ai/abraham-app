    "@fastify/websocket": "^10.0.1",
    "@huggingface/hub": "^0.12.3",
    "@immobiliarelabs/fastify-sentry": "^7.1.0",
    "@mailchimp/mailchimp_transactional": "^1.0.59",
    "@sinclair/typebox": "^0.32.34",
    "ajv": "^8.12.0",
    "archiver": "^7.0.1",
    "aws-sdk": "^2.1454.0",
    "axios": "^1.6.8",
