{
  "name": "api",
  "version": "0.1.0",
  "description": "Eden.art API",
  "type": "module",
  "scripts": {
    "dev:dummy": "DOTENV_PATH=.env.dummy tsx watch src .env.dummy | pino-pretty --colorize",
    "dev:eden": "DOTENV_PATH=.env.eden tsx watch src .env.eden | pino-pretty --colorize",
    "build": "tsc --outDir build",
    "test": "vitest",
    "start": "node --experimental-specifier-resolution=node build/src",
    "pretest": "npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" \"tests/**/*.ts\""
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "nodejs",
    "fastify",
    "typescript"
  ],
  "author": "Eden Labs",
  "license": "MIT",
  "dependencies": {
    "@clerk/clerk-sdk-node": "^5.0.0",
