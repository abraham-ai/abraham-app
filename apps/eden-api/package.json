    "blurhash": "^2.0.5",
    "chromadb": "^1.7.2",
    "cloudinary": "^2.2.0",
    "dotenv": "^16.0.3",
    "fastify": "^4.26.2",
    "fastify-healthcheck": "^4.4.0",
